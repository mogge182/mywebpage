    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="JustMySiteDB" Order="1" Enabled="False">
          <Destination Path="Data Source=justmysite.database.windows.net;Initial Catalog=JustMySiteSQL;Persist Security Info=True;User ID=mogge182;Password=Cool1979" />
